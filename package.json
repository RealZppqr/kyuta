{
  "name": "kyuta",
  "version": "1.0.0",
  "description": "Scalable Discord bot built with TypeScript, ESM, Eris, MongoDB, and Redis",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"tsc && node dist/index.js\"",
